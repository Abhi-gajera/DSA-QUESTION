#include <iostream>
#include <vector>
using namespace std;

int main()
{

    vector<int> arr{3, 0, 1};
    int n = arr.size();

    int sum = 0;

    for (int i = 0; i < n; i++)
    {
        sum = sum + arr[i];
    }

    int fo = (n * (n + 1)) / 2;

    int missing = fo - sum;

    cout << missing << endl;
}
